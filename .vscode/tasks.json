{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "update-includes",
            "command": "Vendor/AutoHotKey/AutoHotkey64.exe",
            "type": "process",
            "args": [
                "Scripts/UpdateIncludes.ahk"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        },
        {
            "label": "build-launchpad",
            "command": "Vendor/AutoHotKey/AutoHotkey64.exe",
            "type": "process",
            "args": [
                "Scripts/Build.ahk"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        },
        {
            "label": "build-overlay-debug",
            "command": "msbuild",
            "type": "process",
            "args": [
                "Launchpad.sln",
                "-property:Configuration=Debug"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$tsc"
        },
        {
            "label": "build-overlay",
            "command": "msbuild",
            "type": "process",
            "args": [
                "Launchpad.sln",
                "-property:Configuration=Release"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$tsc"
        }
    ]
}
